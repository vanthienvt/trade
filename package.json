{
  "name": "crypto-ai-analysis-pro",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd backend && node --watch server.js",
    "dev:frontend": "vite",
    "build": "vite build && npm run copy-404 && npm run copy-nojekyll && npm run check-build",
    "copy-404": "cp 404.html dist/404.html 2>/dev/null || echo '404.html copied'",
    "copy-nojekyll": "cp public/.nojekyll dist/.nojekyll 2>/dev/null || touch dist/.nojekyll || echo '.nojekyll copied'",
    "check-build": "node scripts/check-build.js || true",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^8.2.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
